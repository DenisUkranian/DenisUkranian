@app.on_message(filters.command('check', prefixes='/') & filters.me)
def det_info(_, msg):


    if msg.reply_to_message.from_user.username is None:
         msg.edit(
            f"Name - {msg.reply_to_message.from_user.first_name} {msg.reply_to_message.from_user.last_name}\nID - {msg.reply_to_message.from_user.id}\n \n   f'ğŸ—“ Ğ”Ğ°Ñ‚Ğ° Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ¸: - {time.asctime()}\n' ")

    if msg.reply_to_message.from_user.last_name is None:
        msg.edit(
            f"ğŸ’¼ ĞĞ¸Ğº- {msg.reply_to_message.from_user.first_name}\nğŸ†” ID: - {msg.reply_to_message.from_user.id}\nUsername - @{msg.reply_to_message.from_user.username} \nğŸ—“ Ğ”Ğ°Ñ‚Ğ° Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ¸: - {time.asctime()}\n")

    else:
        msg.edit(f"ğŸ’¼ ĞĞ¸Ğº: -  {msg.reply_to_message.from_user.first_name} {msg.reply_to_message.from_user.last_name}\n"
                 f"ğŸ†” ID:  - <code>{msg.reply_to_message.from_user.id}</code>\n"
                 f"Username - @{msg.reply_to_message.from_user.username}\n"
                 f"ğŸ—“ Ğ”Ğ°Ñ‚Ğ° Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ¸: - {time.asctime()}\n"
                 )
